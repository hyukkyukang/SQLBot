generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/@internal/prisma-second/client"
}

datasource db {
  provider = "postgresql"
  url      = env("SECOND_DATABASE_URL")
}

model cars {
  id         Int     @id
  model      String? @db.VarChar(255)
  horsepower Int?
  max_speed  Int?
  year       Int?
  price      Float?
}
